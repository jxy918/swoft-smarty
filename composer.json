{
    "name": "jxy918/swoft-smarty",
    "description": "smarty view base on swoft",
    "type": "library",
    "version": "v1.0.2",
    "require": {
        "smarty/smarty": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "jxy918",
            "email": "jxy918@163.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swoft\\Smarty\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\Smarty\\": "test/unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml"
    }
